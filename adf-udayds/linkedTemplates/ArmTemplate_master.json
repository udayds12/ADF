{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-udayds"
		},
		"Blobsource_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Blobsource'"
		},
		"SqlDestination_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlDestination'"
		},
		"BlobContainer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BlobContainer'"
		},
		"Sqlserver_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Sqlserver'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-udayds_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Blobsource_connectionString": {
						"value": "[parameters('Blobsource_connectionString')]"
					},
					"SqlDestination_connectionString": {
						"value": "[parameters('SqlDestination_connectionString')]"
					},
					"BlobContainer_connectionString": {
						"value": "[parameters('BlobContainer_connectionString')]"
					},
					"Sqlserver_connectionString": {
						"value": "[parameters('Sqlserver_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}